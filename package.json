    "hugo:build": "hugo mod clean --all && hugo mod get -u && hugo mod tidy && hugo mod npm pack && npm install && attr -s com.dropbox.ignored -V 1 node_modules && attr -s com.dropbox.ignored -V 1 public && attr -s com.dropbox.ignored -V 1 resources && hugo --minify",
    "hugo:dev": "hugo mod clean --all && hugo server --minify --disableFastRender --navigateToChanged --templateMetrics --templateMetricsHints --buildDrafts --buildExpired --buildFuture --watch --forceSyncStatic -D",
    "hugo:server": "hugo mod clean --all && attr -s com.dropbox.ignored -V 1 node_modules && attr -s com.dropbox.ignored -V 1 public && attr -s com.dropbox.ignored -V 1 resources && hugo server --minify --disableFastRender --navigateToChanged --templateMetrics --templateMetricsHints --buildFuture --watch --forceSyncStatic --poll 700ms"
